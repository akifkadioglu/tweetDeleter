{
    "manifest_version": 3,
    "name": "Remove Video Tweets",
    "version": "1.0",
    "description": "video oynatıcılarını otomatik olarak kaldırır.",
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    },
    "content_scripts": [
      {
        "matches": ["https://x.com/*"],
        "js": ["content.js"]
      }
    ],
    "permissions": ["activeTab"]
  }
  